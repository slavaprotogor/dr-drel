pipeline {
	agent any
	stages {
		stage('Build') {
			steps {
				echo 'Building...'
				echo 'Building 1'
				echo 'Building 2'
				echo 'Building 3'
			}
		}
		stage('Test') {
			steps {
				echo 'Testing...'
				echo 'Testing 1'
				echo 'Testing 2'
				echo 'Testing 3'
			}
		}
		stage('Deploy') {
			steps {
				echo 'Deploying...'
				echo 'Deploying 1'
				echo 'Deploying 2'
				echo 'Deploying 3'
			}
		}
	}
}